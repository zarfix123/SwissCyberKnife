#!/home/dennis/Projects/SwissCyberKnife/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cyber-swiss-army','console_scripts','port-scanner'
__requires__ = 'cyber-swiss-army'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cyber-swiss-army', 'console_scripts', 'port-scanner')()
    )
